@model ProductionManagementSystem.Models.Device;

@{
    ViewBag.Title = "Удалить";
}
<h2>Удалить</h2>

<h3>Вы уверены, что хотите удалить это?</h3>

<div class="container">
    @DeviceHelper.CreateDevice(Html, Model, false, null)
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Вернуться к списку конструктива</a>
    </form>
</div>
